<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.unioeste.riscvirtualmachine.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label text="RISC-V Virtual Machine">
      <font>
         <Font name="System Bold" size="24.0" />
      </font>
   </Label>
   <HBox fx:id="header" alignment="CENTER" layoutX="30.0" layoutY="65.0" prefHeight="0.0" spacing="20.0">
      <children>
         <HBox fx:id="header1" alignment="CENTER" prefHeight="0.0" spacing="5.0" HBox.hgrow="NEVER">
            <children>
               <Button fx:id="fileChooserBtn" mnemonicParsing="false" text="Load" />
               <Label text="No file" textFill="#00000087" />
            </children>
         </HBox>
         <Region maxWidth="600.0" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
         <HBox fx:id="header11" alignment="CENTER" layoutX="77.0" layoutY="10.0" prefHeight="0.0" spacing="5.0">
            <children>
               <Button fx:id="fileChooserBtn1" disable="true" mnemonicParsing="false" text="Start" />
               <Button disable="true" mnemonicParsing="false" text="Next" />
            </children>
         </HBox>
      </children>
      <VBox.margin>
         <Insets bottom="10.0" top="10.0" />
      </VBox.margin>
   </HBox>
   <HBox alignment="CENTER" prefHeight="0.0" spacing="20.0">
      <children>
         <Label alignment="CENTER" text="Label" />
         <Label text="Label" />
         <Label text="Label" />
         <Label text="Label" />
         <Label alignment="CENTER" layoutX="86.0" layoutY="52.0" text="Label" />
         <Label layoutX="114.0" layoutY="52.0" text="Label" />
         <Label layoutX="142.0" layoutY="52.0" text="Label" />
      </children>
   </HBox>
   <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0" VBox.vgrow="ALWAYS">
      <children>
         <TableView editable="true" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
           <columns>
             <TableColumn prefWidth="75.0" text="C1" />
             <TableColumn prefWidth="75.0" text="C2" />
           </columns>
         </TableView>
         <ListView prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
      </children></HBox>
</VBox>
